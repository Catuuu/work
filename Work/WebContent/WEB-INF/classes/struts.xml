    <action name="addStudent.do" class="com.cheea.action.AddStudentByAdmin"></action>
    <action name="updateStudent.do" class="com.cheea.action.UpdateStudentByAdmin"></action>
    <action name="deleteStudent.do" class="com.cheea.action.DeleteStudentByAdmin"></action>
    <action name="addTeacher.do" class="com.cheea.action.AddTeacherByAdmin"></action>
    <action name="updateTeacher.do" class="com.cheea.action.UpdateTeacherByAdmin"></action>
    <action name="deleteTeacher.do" class="com.cheea.action.DeleteTeacherByAdmin"></action>
    <action name="addClass.do" class="com.cheea.action.AddClassByAdmin"></action>
    <action name="updateClass.do" class="com.cheea.action.UpdateClassByAdmin"></action>
