    <action name="search.do" class="com.cheea.action.SearchReadyByAdmin"></action>
    <action name="exit.do" class="com.cheea.action.Exit">
      <forward name="success" path="login.jsp" redirect="false"></forward>
      <forward name="fail" path="error.jsp" redirect="false"></forward>
    </action>
    <action name="addUser.do" class="com.cheea.action.AddUserByAdmin">
      <forward name="success" path="getAll.do" redirect="false"></forward>
      <forward name="fail" path="error.jsp" redirect="false"></forward>
    </action>
</struts-mapping>