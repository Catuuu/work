    <action name="getstu.do" class="com.cheea.action.GetStudentByAdmin"></action>
    <action name="getcal.do" class="com.cheea.action.GetClassByAdmin"></action>
    <action name="gettea.do" class="com.cheea.action.GetTeacherByAdmin"></action>
    <action name="getcou.do" class="com.cheea.action.GetCourseByAdmin"></action>
    <action name="getrea.do" class="com.cheea.action.GetReadyClassByAdmin"></action>
    <action name="getfal.do" class="com.cheea.action.GetFailClassByAdmin"></action>  
    <action name="gettime.do" class="com.cheea.action.GetTimeByAdmin"></action>  
    <action name="getxsl.do" class="com.cheea.action.GetXsl"></action>
